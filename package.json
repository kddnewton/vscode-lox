{
  "name": "vscode-lox",
  "version": "1.0.0",
  "description": "VSCode support for the lox language",
  "main": "lox.ts",
  "author": "Kevin Newton",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.14",
    "ts-node": "^10.4.0",
    "typescript": "^4.1.2"
  },
  "contributes": {
    "languages": [
      {
        "id": "lox",
        "aliases": ["lox"],
        "extensions": [".lox"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "lox",
        "scopeName": "source.lox",
        "path": "./syntaxes/lox.tmLanguage.json"
      }
    ]
  }
}
